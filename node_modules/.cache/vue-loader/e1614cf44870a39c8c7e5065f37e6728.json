/td>\n            <td>{{ flat.zipcode }}</td>\n            <td>{{ flat.flatrooms }}</td>\n            <!-- <td class=\"button\">\n              <router-link\n                :to=\"{name: 'edituser', params: { id: flat.key }}\"\n                class=\"btn btn-primary\"\n              >Edit</router-link>\n              <button @click.prevent=\"deleteUser(flat.key)\" class=\"btn btn-danger\">Delete</button>\n              <router-link\n                :to=\"{name: 'detailuser', params: { id: flat.key }}\"\n                class=\"btn btn-info\"\n              >Details</router-link>\n            </td>-->\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { db } from \"../firebaseDb\";\n\nexport default {\n  data() {\n    return {\n      Flats: [],\n      applicants: [\n        {\n          number: \"\",\n          area: \"\",\n          price: \"\"\n        }\n      ]\n    };\n  },\n  created() {\n    db.collection(\"flats\").onSnapshot(snapshotChange => {\n      this.Flats = [];\n      snapshotChange.forEach(doc => {\n        this.Flats.push({\n          key: doc.id,\n          idflat: doc.data().idflat,\n          numberflat: doc.data().numberflat,\n          nameflat: doc.data().nameflat,\n          street: doc.data().street,\n          zipcode: doc.data().zipcode,\n          flatrooms: doc.data().flatrooms\n        });\n      });\n    });\n  },\n  methods: {\n    deleteUser(id) {\n      if (window.confirm(\"Do you really want to delete?\")) {\n        db.collection(\"flats\")\n          .doc(id)\n          .delete()\n          .then(() => {\n            console.log(\"Document deleted!\");\n          })\n          .catch(error => {\n            console.error(error);\n          });\n      }\n    }\n  }\n};\n</script>\n\n<style scope>\n.button {\n  text-align: center;\n}\n\n.btn {\n  margin-right: 12px;\n  margin-left: 12px;\n}\n.container {\n  padding-top: 70px;\n}\n\nh1 {\n  text-align: center;\n}\n</style>\n\n"]}]}